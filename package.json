{
  "name": "eleventy-starter-parcel",
  "version": "1.0.0",
  "description": "An @11ty/eleventy starter using parcel-bundler for production bundling",
  "author": "chrisdmacrae",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "prestart": "run-s clean pulp:build 11ty:build",
    "start": "run-p develop:*",
    "dev": "npm run start",
    "repl": "run-p pulp:repl",
    "prebuild": "run-p clean",
    "build": "run-s build:*",
    "build:generate": "run-s pulp:build 11ty:build parcel:build",
    "develop:generate": "run-p pulp:watch 11ty:watch parcel:watch",
    "develop:server": "browser-sync start --config .bs-config.js",
    "clean": "run-p clean:*",
    "clean:cache": "rimraf .tmp",
    "clean:dist": "rimraf dist",
    "clean:pure": "rimraf src/pages/assets/js/pure",
    "11ty:build": "eleventy  --quiet --output .tmp/parcel",
    "11ty:watch": "ELEVENTY_ENV=development eleventy  --quiet --watch --output .tmp/11ty",
    "parcel:build": "parcel build .tmp/parcel/*.html --out-dir dist --cache-dir .tmp/.cache",
    "parcel:watch": "ELEVENTY_ENV=development parcel serve .tmp/parcel/*.html --out-dir dist --cache-dir .tmp/.cache",
    "pulp:repl": "pulp --psc-package repl",
    "pulp:build": "pulp --psc-package build -o src/pages/assets/js/pure --no-check-main --skip-entry-point",
    "pulp:watch": "pulp --psc-package --watch build -o src/pages/assets/js/pure --no-check-main --skip-entry-point ",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "alias": {
    "pages": "./src/pages",
    "includes": "./src/includes",
    "data": "./src/data",
    "css": "./src/pages/assets/css",
    "js": "./src/pages/assets/js"
  },
  "dependencies": {
    "@11ty/eleventy": "^0.8.0",
    "@fullhuman/postcss-purgecss": "^1.1.0",
    "autoprefixer": "^9.5.0",
    "cash-cp": "^0.2.0",
    "chokidar-cli": "^1.2.1",
    "date-fns": "^2.0.0-alpha.27",
    "fs-extra": "^7.0.1",
    "globby": "^9.0.0",
    "htm": "^2.1.1",
    "npm-run-all": "^4.1.5",
    "nunjucks": "^3.1.7",
    "parcel-bundler": "^1.12.3",
    "postcss-easy-import": "^3.0.0",
    "rimraf": "^2.6.3",
    "tailwindcss": "^0.7.4",
    "workbox-build": "^4.2.0"
  },
  "devDependencies": {
    "browser-sync": "^2.26.3",
    "prettier": "^1.16.4",
    "parcel-plugin-copy-unbundled": "file:lib/parcel/parcel-plugin-copy-unbundled",
    "parcel-plugin-eleventy-sync": "file:lib/parcel/parcel-plugin-eleventy-sync",
    "parcel-plugin-nunjucks-precompile": "file:lib/parcel/parcel-plugin-nunjucks-precompile",
    "parcel-plugin-service-worker": "file:lib/parcel/parcel-plugin-service-worker"
  }
}
